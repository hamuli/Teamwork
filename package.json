{
    "name": "teamwork",
    "version": "1.0.0",
    "description": "this work is for andela bootcamp challenge",
    "main": "index.js",
    "scripts": {
        "dev-start": "nodemon index --exec babel-node --presets @babel/preset-env",
        "test": " nyc _mocha ./Server/test/*.js --recursive --require @babel/polyfill  --require @babel/register --no-timeout --exit",
        "generate-lcov": "nyc --reporter=lcov --reporter=text-lcov npm test | coveralls",
        "coverage": "nyc npm test && npm run generate-lcov",
        "codeclimate-coverage": "codeclimate-test-reporter < lcov.info",
        "build": "babel index.js --out-dir build",
        "start": "babel-node index.js"
    },
    "nyc": {
        "require": [
            "@babel/register"
        ],
        "sourceMap": true,
        "instrument": true
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hamuli/Teamwork.git"
    },
    "keywords": [
        "teamwork"
    ],
    "author": "glodie hamuli",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/hamuli/Teamwork/issues"
    },
    "homepage": "https://github.com/hamuli/Teamwork#readme",
    "dependencies": {
        "@babel/cli": "^7.6.2",
        "@babel/core": "^7.6.2",
        "@babel/node": "^7.6.2",
        "@babel/plugin-transform-runtime": "^7.6.2",
        "@babel/polyfill": "^7.6.0",
        "@babel/preset-env": "^7.6.2",
        "@babel/register": "^7.6.2",
        "@babel/runtime": "^7.6.2",
        "append-transform": "^2.0.0",
        "babel-node": "0.0.1-security",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "brace": "^0.11.1",
        "chai": "^4.2.0",
        "chai-http": "^4.3.0",
        "coveralls": "^3.0.6",
        "cross-env": "^6.0.0",
        "dotenv": "^8.1.0",
        "express": "^4.17.1",
        "express-validator": "^6.2.0",
        "jsonwebtoken": "^8.5.1",
        "mocha": "^6.2.1",
        "mocha-lcov-reporter": "^1.3.0",
        "morgan": "^1.9.1",
        "nyc": "^14.1.1"
    },
    "devDependencies": {
        "brace": "^0.11.1",
        "chai": "^4.2.0",
        "chai-http": "^4.3.0",
        "cross-env": "^6.0.0",
        "eslint": "^6.4.0",
        "mocha": "^6.2.1",
        "mocha-lcov-reporter": "^1.3.0",
        "nodemon": "^1.19.2",
        "nyc": "^14.1.1"
    }
}
